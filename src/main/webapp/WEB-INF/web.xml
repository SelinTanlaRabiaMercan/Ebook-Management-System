<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
  <servlet>
    <servlet-name>KayitServlet</servlet-name>
    <servlet-class>com.user.servlet.KayitServlet</servlet-class>
  </servlet>
   <servlet>
  <servlet-name>LoginServlet</servlet-name>
  <servlet-class>com.user.servlet.LoginServlet</servlet-class>
</servlet>
  
  <!-- servlet mapping for kayitServlet -->
  <servlet-mapping>
    <servlet-name>KayitServlet</servlet-name>
    <url-pattern>/KayitServlet</url-pattern>
  </servlet-mapping>
  
  <!-- servlet mapping for loginServlet -->

<servlet-mapping>
  <servlet-name>LoginServlet</servlet-name>
  <url-pattern>/LoginServlet</url-pattern>
</servlet-mapping>
</web-app>
  
